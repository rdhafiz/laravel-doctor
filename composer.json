{
    "name": "codevioso/laravel-doctor",
    "description": "Lightweight diagnostic tool for Laravel apps",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "diagnostics",
        "cli",
        "artisan",
        "performance"
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "symfony/console": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Codevioso\\LaravelDoctor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codevioso\\LaravelDoctor\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codevioso\\LaravelDoctor\\DoctorServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
